{
   "background": {
      "service_worker": "background.js"
   },
   "description": "Easily open desired links in a PDF viewer.",
   "homepage_url": "https://mybrowseraddon.com/open-in-pdf.html",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Open in PDF Viewer",
   "offline_enabled": true,
   "options_ui": {
      "open_in_tab": true,
      "page": "data/options/options.html"
   },
   "permissions": [ "storage", "downloads", "contextMenus", "notifications", "nativeMessaging" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.4"
}
